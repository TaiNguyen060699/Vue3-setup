{
  "root": true,
  "env": {
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "@vue/eslint-config-typescript",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "rules": {
    "vue/multi-word-component-names": "off"
  },
  "ignorePatterns": [
    "node_modules/*",
    "dist/*",
    "public/*"
  ]
}
